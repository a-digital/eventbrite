{
  "name": "adigital/eventbrite",
  "description": "Integration with Eventbrite API",
  "type": "craft-plugin",
  "version": "2.0.2",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "eventbrite"
  ],
  "support": {
    "docs": "https://github.com/a-digital/eventbrite/blob/master/README.md",
    "issues": "https://github.com/a-digital/eventbrite/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "A Digital",
      "homepage": "https://adigital.agency/"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "adigital\\eventbrite\\": "src/"
    }
  },
  "extra": {
    "name": "Eventbrite",
    "handle": "eventbrite",
    "hasCpSettings": true,
    "hasCpSection": true,
    "changelogUrl": "https://raw.githubusercontent.com/a-digital/eventbrite/master/CHANGELOG.md",
    "components": {
      "eventbriteEvents": "adigital\\eventbrite\\services\\EventbriteEvents"
    },
    "class": "adigital\\eventbrite\\Eventbrite"
  },
  "config": {
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
